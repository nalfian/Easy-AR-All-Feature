<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ndroid="http://schemas.android.com/apk/distribution"
    package="cn.easyar.samples.helloarcloud">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        ndroid:usesCleartextTraffic="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cn.easyar.samples.helloarcloud.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cn.easyar.samples.helloarcloud.ARActivity" android:screenOrientation="unspecified" android:configChanges="orientation|keyboardHidden|screenSize|smallestScreenSize" />
    </application>
</manifest>
